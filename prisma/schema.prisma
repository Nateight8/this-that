// prisma/schema.prisma

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

enum Gender {
  MAN
  WOMAN
}

model CreatePickSlip {
  id          String   @id @default(uuid()) 
  gender      Gender  
  name        String
  partnerName String
  first       String
  second      String
  createdAt   DateTime @default(now())
}
